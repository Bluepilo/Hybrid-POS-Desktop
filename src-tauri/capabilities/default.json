{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "Capability for the main window",
	"windows": ["main"],
	"permissions": [
		"core:default",
		"opener:default",
		"sql:default",
		"sql:allow-load",
		"sql:allow-execute",
		"sql:allow-select",
		"sql:allow-close",
		"dialog:default",
		"dialog:allow-confirm",
		{
			"identifier": "http:default",
			"allow": ["https://www.google.com", "https://dev-app.bluepilo.com"]
		}
	]
}
